<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sqisland.android.animated_vector_drawable" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
      <activity
          android:name=".ExampleActivity"
          android:label="@string/example_from_documentation"
          android:parentActivityName=".MainActivity" />
      <activity
          android:name=".RotateActivity"
          android:label="@string/clock"
          android:parentActivityName=".MainActivity" />
      <activity
          android:name=".PathMorphActivity"
          android:label="@string/smiling_face"
          android:parentActivityName=".MainActivity" />
      <activity
          android:name=".FillInHeartActivity"
          android:label="@string/fill_in_heart"
          android:parentActivityName=".MainActivity" />
    </application>

</manifest>